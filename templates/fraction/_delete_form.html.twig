<form method="post" action="{{ path('app_fraction_delete', {'id': fraction.id}) }}" onsubmit="return confirm('Czy na pewno chcesz usunąć tę frakcję?');" class="delete_form">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ fraction.id) }}">
    <button class="btn btn-danger">{{ button_label|default('Usuń') }}</button>
</form>
