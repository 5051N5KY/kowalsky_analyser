{% extends 'base.html.twig' %}

{% block title %}ScannedObject{% endblock %}

{% block body %}
    <h1>ScannedObject</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ scanned_object.id }}</td>
            </tr>
            <tr>
                <th>Photo_path</th>
                <td>{{ scanned_object.photoPath }}</td>
            </tr>
            <tr>
                <th>Pdf_path</th>
                <td>{{ scanned_object.pdfPath }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ scanned_object.name }}</td>
            </tr>
            <tr>
                <th>General_description</th>
                <td>{{ scanned_object.generalDescription }}</td>
            </tr>
            <tr>
                <th>Background_description</th>
                <td>{{ scanned_object.backgroundDescription }}</td>
            </tr>
            <tr>
                <th>Analysis_result</th>
                <td>{{ scanned_object.analysisResult }}</td>
            </tr>
            <tr>
                <th>Who_brought_object_name_id</th>
                <td>{{ scanned_object.whoBroughtObjectNameId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_scanned_object_index') }}">back to list</a>

    <a href="{{ path('app_scanned_object_edit', {'id': scanned_object.id}) }}">edit</a>

    {{ include('scanned_object/_delete_form.html.twig') }}
{% endblock %}
